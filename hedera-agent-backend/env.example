# Hedera configuration
HEDERA_NETWORK=testnet
HEDERA_OPERATOR_ID=0.0.1234
HEDERA_OPERATOR_KEY=302e020100300506032b657004220420exampleexampleexampleexampleexampleexampleexampleexampleexample
# Fallback environment variable names when using the deployment script
HEDERA_ACCOUNT_ID=0.0.1234
HEDERA_PRIVATE_KEY=302e020100300506032b657004220420exampleexampleexampleexampleexampleexampleexampleexampleexample

# Deployment-time configuration
# Leave AGENT_VERIFIER_ADDRESS blank to deploy a development stub verifier.
AGENT_VERIFIER_ADDRESS=
AGENT_PAYMENT_TARGET=0x0000000000000000000000000000000000000000

# Runtime configuration for the orchestrator
AGENT_CONTRACT_ID=0.0.5678

# Optional tuning
HEDERA_CONTRACT_GAS_LIMIT=2000000
FUNDING_POLL_INTERVAL_MS=300000

# When true, contract executions are skipped and only logged
HEDERA_AGENT_DRY_RUN=true

# vlayer configuration
VLAYER_ENABLED=false
VLAYER_CLI_PATH=vlayer
VLAYER_NOTARY_URL=https://test-notary.vlayer.xyz/
VLAYER_JWT_TOKEN=
VLAYER_PROOF_URL=

# Optional deployment overrides
CONTRACT_ARTIFACTS_DIR=/absolute/path/to/my-web-proof/out
DEPLOY_GAS_LIMIT=2000000

